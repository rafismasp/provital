<% case params[:partial] %>
<% when 'filter_select' %>
	$('#index_content').html("<%= escape_javascript(render 'partial') %>");	
<% when 'change_filter_column' %>
	$('#filter_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'change_supplier' %>
	$('.select_taxes').html("<%= escape_javascript(render 'partial', {field:'tax_id'}) %>");
	$('.select_currencies').html("<%= escape_javascript(render 'partial', {field:'currency_id'}) %>");
	$('.select_term_of_payments').html("<%= escape_javascript(render 'partial', {field:'term_of_payment_id'}) %>");
	$('.select_top_day').val("<%= params[:select_top_day] %>");
	$('.select_tax_rates').html("<%= escape_javascript(render 'partial', {field:'tax_rate_id'}) %>");
	$('#select_invoice_suppliers').html("<%= escape_javascript(render 'partial', {field:'modal_invoice_supplier'}) %>");

	$('.button-grn-modal').html("<a class='uk-button uk-button-primary uk-button-small' href='#invoice-sections' uk-toggle>Select Invoice</a>");
<% when 'load_invoice_supplier' %>
	$('#item').html("<%= escape_javascript(render 'partial') %>");	
	$("#payment_request_supplier_subtotal").val(parseFloat("<%= @invoice_suppliers.sum(:subtotal) %>"));
	$("#payment_request_supplier_ppntotal").val(parseFloat("<%= @invoice_suppliers.sum(:ppntotal) %>"));
	$("#payment_request_supplier_pphtotal").val(parseFloat("<%= @invoice_suppliers.sum(:pphtotal) %>"));
	$("#payment_request_supplier_dptotal").val(parseFloat("<%= @invoice_suppliers.sum(:dptotal) %>"));
	$("#payment_request_supplier_grandtotal").val(parseFloat("<%= @invoice_suppliers.sum(:grandtotal) %>"));
<% else %>
	$('#content').html("<%= escape_javascript(render 'index') %>");
<% end %>

