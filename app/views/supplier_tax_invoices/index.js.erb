<% case params[:partial] %>
<% when 'filter_select' %>
	$('#index_content').html("<%= escape_javascript(render 'partial') %>");	
<% when 'change_filter_column' %>
	$('#filter_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'change_supplier' %>
	$('.select_currencies').html("<%= escape_javascript(render 'partial', {field:'currency_id'}) %>");
<% when 'change_fp_number' %>
	$("#item").html("<%= escape_javascript(render 'partial') %>");
	<% dpptotal = 0 %>
	<% ppntotal = 0 %>
	<% @invoice_suppliers.each do |invoice| %>
		<% dpptotal += invoice.subtotal.to_f %>
		<% ppntotal += invoice.ppntotal.to_f %>
	<% end if @invoice_suppliers.present? %>
	$("#supplier_tax_invoice_dpptotal").val("<%= number_with_precision(dpptotal, precision: 0, delimiter: ".", separator: ",") %>");
	$("#supplier_tax_invoice_ppntotal").val("<%= number_with_precision(ppntotal, precision: 0, delimiter: ".", separator: ",") %>");
<% else %>
	$('#content').html("<%= escape_javascript(render 'index') %>");
<% end %>