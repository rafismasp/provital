
<div class="uk-container uk-container-expand">        
  <div class="uk-grid uk-grid-medium  " data-uk-grid>
    
    <!-- panel -->
    <div class="uk-width-1-1@m">
      <div class="uk-card uk-card-default uk-card-small uk-card-hover">
        
        <div class="uk-card-header">
          <div class="uk-grid uk-grid-small">
            <div class="uk-width-auto" title="<%= feature("description") %>"><h4><%= feature("name").humanize %></h4></div>
          </div>
          <hr class="uk-margin-small">

          <nav class="uk-navbar-container uk-navbar-transparent" uk-navbar>
            <div class="uk-navbar-left">
              <div class="uk-padding-small">
                <%= render 'layouts/form_item', c: 1, field_name: "product_batch_number_id", field_value: "" %>
              </div>
              <div class="uk-padding-small">
              </div>
              <div class="uk-padding-small">
              </div>
              <div class="uk-padding-small">
              </div>
              <div class="uk-padding-small">
              </div>
                
            </div>
            <div class="uk-navbar-right">
              <div class="uk-padding-small">
              </div>
            </div>
          </nav>

        </div>

        <div class="uk-card-body uk-background-muted">
          <div class="uk-overflow-auto" id="index_content">
            <%= render 'partial' %>
          </div>
        </div>
      </div>
    </div>
    <!-- /panel -->
    <%#= render 'layouts/filter' %>
  </div>
  
</div>
