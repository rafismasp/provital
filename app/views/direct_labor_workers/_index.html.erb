
<div class="uk-container uk-container-expand">        
  <div class="uk-grid uk-grid-medium  " data-uk-grid>
    
    <!-- panel -->
    <div class="uk-width-1-1@m">
      <div class="uk-card uk-card-default uk-card-small uk-card-hover">
        <div class="uk-card-header">
          <div class="uk-grid uk-grid-small">
            <div class="uk-width-auto" title="<%= feature("description") %>"><h4><%= feature("name") %></h4></div>
          </div>
          <hr class="uk-margin-small">

          <nav class="uk-navbar-container uk-navbar-transparent" uk-navbar>
            <div class="uk-navbar-left">
              <div class="uk-padding-small">
                <%= link_to 'ADD', new_direct_labor_worker_path, class: "uk-button uk-button-primary uk-button-small" %>
              </div>
              <div class="uk-padding-small">
                <a class="uk-button uk-button-default uk-button-small" href="#modal-sections" uk-toggle>Filter</a>
              </div>
              
            </div>
            <div class="uk-navbar-right">
              <div class="uk-padding-small">
                <%= link_to 'Export', 'javascript:;', class: "uk-button uk-button-primary uk-button-small", :q=> params[:q],
                :onclick=> "export_report(this, '"+params[:controller].to_s+"')" %>
              </div>
            </div>
          </nav>
          
        </div>
        <div class="uk-card-body">
          <div class="uk-overflow-auto" id="index_content">
            <%= render 'partial' %>
          </div>
        </div>
      </div>
    </div>
    <!-- /panel -->
    <%= render 'layouts/filter' %>

  </div>
  
</div>


