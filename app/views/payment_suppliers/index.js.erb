<% case params[:partial] %>
<% when 'filter_select' %>
	$('#index_content').html("<%= escape_javascript(render 'partial') %>");	
<% when 'change_filter_column' %>
	$('#filter_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'change_supplier' %>
	$('.select_taxes').html("<%= escape_javascript(render 'partial', {field:'tax_id'}) %>");
	$('.select_currencies').html("<%= escape_javascript(render 'partial', {field:'currency_id'}) %>");
	$('.select_term_of_payments').html("<%= escape_javascript(render 'partial', {field:'term_of_payment_id'}) %>");
	$('.select_top_day').val("<%= params[:select_top_day] %>");
	$('.select_tax_rates').html("<%= escape_javascript(render 'partial', {field:'tax_rate_id'}) %>");
	$('#select_payment_request_suppliers').html("<%= escape_javascript(render 'partial', {field:'modal_payment_request_supplier'}) %>");

	$('.button-grn-modal').html("<a class='uk-button uk-button-primary uk-button-small' href='#payreq-sections' uk-toggle>Select Payreq</a>");
<% when 'load_payment_request_supplier' %>
	$('#item').html("<%= escape_javascript(render 'partial') %>");
	
	$("#payment_supplier_subtotal").val(parseFloat("<%= @payment_request_suppliers.sum(:subtotal) %>"));
	$("#payment_supplier_ppntotal").val(parseFloat("<%= @payment_request_suppliers.sum(:ppntotal) %>"));
	$("#payment_supplier_pphtotal").val(parseFloat("<%= @payment_request_suppliers.sum(:pphtotal) %>"));
	$("#payment_supplier_dptotal").val(parseFloat("<%= @payment_request_suppliers.sum(:dptotal) %>"));
	$("#payment_supplier_grandtotal").val(parseFloat("<%= @payment_request_suppliers.sum(:grandtotal) %>"));
<% else %>
	$('#content').html("<%= escape_javascript(render 'index') %>");
<% end %>

