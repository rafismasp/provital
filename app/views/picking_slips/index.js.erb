<% case params[:partial] %>
<% when 'filter_select' %>
	$('#index_content').html("<%= escape_javascript(render 'partial') %>");	
<% when 'change_filter_column' %>
	$('#filter_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'change_customer' %>
	$('.sales_order_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'change_sales_order' %>
	$('.product_detail').html("<%= escape_javascript(render 'layouts/form_item', c: 0, field_name: 'product_id', field_value: '') %>  ");
	$('#select_product_batch_number').html("");
	$('#item').html("<span class='uk-badge'>Please Select Product</span>");
<% when 'change_product' %>
	$('#select_product_batch_number').html("<%= escape_javascript(render 'partial') %>");
	// $('#item').html("<span class='uk-badge'>Please Select Product Batch Number</span>");
<% when 'load_product_batch_number' %>
	$('#item').html("<%= escape_javascript(render 'partial') %>");
<% when 'add_item' %>
	$('#item tr:last').after("<%= escape_javascript(render 'partial') %>");
<% else %>
	$('#modal-sections').remove();
	$('#content').html("<%= escape_javascript(render 'index') %>");
<% end %>