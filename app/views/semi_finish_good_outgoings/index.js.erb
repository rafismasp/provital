<% case params[:partial] %>
<% when 'filter_select' %>
	$('#index_content').html("<%= escape_javascript(render 'partial') %>");	
<% when 'change_filter_column' %>
	$('#filter_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'add_item' %>
	$('#item tr:last').after("<%= escape_javascript(render 'partial') %>");
<% when 'change_product_batch_number' %>
	$(".sfo_number<%= params[:c] %>").html("<%= escape_javascript(render 'partial', {kind: 'sfo_number'}) %>");
	$(".sfos_number<%= params[:c] %>").html("<%= escape_javascript(render 'partial', {kind: 'sfos_number'}) %>");
	<% if @sfo_number.present? %>
		$(".quantity<%= params[:c] %>").val("<%= @sfo_number.outstanding_sterilization_out.to_i %>");
	<% end %>
<% else %>
	$('#modal-sections').remove();
	$('#content').html("<%= escape_javascript(render 'index') %>");
<% end %>