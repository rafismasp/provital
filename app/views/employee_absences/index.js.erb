<% case params[:partial] %>
<% when 'filter_select' %>
	$('#index_content').html("<%= escape_javascript(render 'partial') %>");	
<% when 'change_filter_column' %>
	$('#filter_detail').html("<%= escape_javascript(render 'partial') %>");
<% when 'change_employee' %>
	$('.employee_nik').html("<%= escape_javascript(render 'partial', {field:'nik'}) %>");
	$('#item').html("<%= escape_javascript(render 'partial', {field:'tb_item'}) %>");
<% when 'approve_all' %>
	$('#content').html("<%= escape_javascript(render 'approve_all') %>");
<% when 'get_total_day' %>
	$('.total_day').html("<%= escape_javascript(render 'partial') %>");
<% else %>
	$('#modal-sections').remove();
	$('#content').html("<%= escape_javascript(render 'index') %>");
<% end %>